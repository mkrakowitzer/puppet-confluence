{
  "name": "mkrakowitzer-confluence",
  "version": "2.1.1",
  "author": "mkrakowitzer",
  "summary": "Install atlassian confluence",
  "license": "MIT",
  "source": "https://github.com/puppet-community/puppet-confluence.git",
  "project_page": "https://github.com/puppet-community/puppet-confluence",
  "issues_url": "https://github.com/puppet-community/puppet-confluence/issues",
  "description": "Atlassian confluence",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0"
    },
    {
      "name": "mkrakowitzer/deploy",
      "version_requirement": ">= 0.0.3"
    },
    {
      "name": "nanliu/staging",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.4.0 <4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ]
}
